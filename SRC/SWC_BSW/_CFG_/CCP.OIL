};


/******************************************************************************/
/* CCP MEMORY CONFIGURATION                                                   */
/******************************************************************************/

CCP_WRITABLE_MEMORY_ZONE  ROM_CAL
{
    ADDRESS_START = "0x80060000";
    ADDRESS_STOP  = "0x8006FFFF";
};

CCP_WRITABLE_MEMORY_ZONE  RAM_CPU0 /* corresponds to DSPR0 */
{
    ADDRESS_START = "0x70000000";
    ADDRESS_STOP  = "0x7002DFFF";
};

CCP_WRITABLE_MEMORY_ZONE  LOCAL_RAM /* corresponds to LMU RAM */
{
    ADDRESS_START = "0xB0000000";
    ADDRESS_STOP  = "0xB0008000";
};

CCP_WRITABLE_MEMORY_ZONE  EMEM /* corresponds to ED MEM */
{
    ADDRESS_START = "0xBF000000";
    ADDRESS_STOP  = "0xBF100000";
};

CCP_WRITABLE_MEMORY_ZONE  REGISTERS /* corresponds to Segment 15 */
{
    ADDRESS_START = "0xF0000000";
    ADDRESS_STOP  = "0xFF120000";
};


CCP_READ_ONLY_MEMORY_ZONE ROM_CACHED /* corresponds to PF0 & PF1 */
{
    ADDRESS_START = "0x80000000";
    ADDRESS_STOP  = "0x801FFFFF";
};

CCP_READ_ONLY_MEMORY_ZONE  ROM  /* corresponds to PF0 & PF1 */
{
    ADDRESS_START = "0xA0000000";
    ADDRESS_STOP  = "0xA01FFFFF";
};

CCP_READ_ONLY_MEMORY_ZONE  DATA_ROM_DF0 /* corresponds to DF0 */
{
    ADDRESS_START = "0xAF000000";
    ADDRESS_STOP  = "0xAF100000";
};


